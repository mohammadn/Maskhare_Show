ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AIFlatSwitch" "${PODS_CONFIGURATION_BUILD_DIR}/AMPopTip" "${PODS_CONFIGURATION_BUILD_DIR}/BFPaperButton" "${PODS_CONFIGURATION_BUILD_DIR}/Hero" "${PODS_CONFIGURATION_BUILD_DIR}/LRSpotlight" "${PODS_CONFIGURATION_BUILD_DIR}/SwiftMessages" "${PODS_CONFIGURATION_BUILD_DIR}/TGPControls"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AIFlatSwitch/AIFlatSwitch.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/AMPopTip/AMPopTip.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/BFPaperButton/BFPaperButton.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/Hero/Hero.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/LRSpotlight/LRSpotlight.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/SwiftMessages/SwiftMessages.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/TGPControls/TGPControls.framework/Headers"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_LDFLAGS = $(inherited) -framework "AIFlatSwitch" -framework "AMPopTip" -framework "BFPaperButton" -framework "Foundation" -framework "Hero" -framework "LRSpotlight" -framework "SwiftMessages" -framework "TGPControls" -framework "UIKit"
OTHER_SWIFT_FLAGS = $(inherited) -D COCOAPODS
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
